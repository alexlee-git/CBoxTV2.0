<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <tv.newtv.cboxtv.views.custom.RecycleImageView
        android:id="@+id/id_module_28_title_icon"
        android:layout_width="@dimen/width_17px"
        android:layout_height="@dimen/height_17px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_56px"
        android:background="@drawable/tuoyuan"
        android:tag="module_028_title_icon"
        android:visibility="gone" />

    <TextView
        android:id="@+id/id_module_28_title"
        style="@style/ModuleTitleStyle"
        android:layout_width="@dimen/width_376px"
        android:layout_height="@dimen/height_34px"
        android:layout_marginLeft="@dimen/width_6px"
        android:layout_marginTop="@dimen/height_47px"
        android:layout_toRightOf="@id/id_module_28_title_icon"
        android:includeFontPadding="false"
        android:tag="module_028_title"
        android:visibility="gone" />

    <!-- 1 -->
    <FrameLayout
        android:id="@+id/id_module_28_view1"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_228px"
        android:layout_below="@id/id_module_28_title"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_028_1">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_240px"
            android:layout_height="@dimen/height_162px"
            android:tag="cell_028_1_poster" />

    </FrameLayout>


    <!-- 2 -->
    <FrameLayout
        android:id="@+id/id_module_28_view2"
        android:layout_width="@dimen/width_2458px"
        android:layout_height="@dimen/height_228px"
        android:layout_below="@id/id_module_28_title"
        android:layout_marginLeft="-18px"
        android:layout_marginTop="@dimen/height_15px"
        android:layout_toRightOf="@id/id_module_28_view1"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_028_2">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_1392px"
            android:layout_height="@dimen/height_162px"
            android:tag="cell_028_2_poster" />

    </FrameLayout>

</RelativeLayout>


