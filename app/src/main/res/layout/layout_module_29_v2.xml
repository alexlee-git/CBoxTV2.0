<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:id="@+id/id_module_29_title_icon"
            android:layout_width="@dimen/width_17px"
            android:layout_height="@dimen/height_17px"
            android:layout_marginLeft="@dimen/width_20px"
            android:layout_marginTop="@dimen/height_68px"
            android:background="@drawable/tuoyuan"
            android:tag="module_029_title_icon"
            android:visibility="gone" />

        <TextView
            android:id="@+id/id_module_29_title"
            style="@style/ModuleTitleStyle"
            android:layout_width="@dimen/width_1600px"
            android:layout_height="@dimen/height_34px"
            android:layout_marginLeft="@dimen/width_8px"
            android:layout_marginTop="@dimen/height_59px"
            android:layout_toRightOf="@id/id_module_29_title_icon"
            android:includeFontPadding="false"
            android:tag="module_029_title"
            android:visibility="gone" />

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_title"
            android:layout_marginTop="@dimen/height_15px"
            app:block_poster_height="@dimen/height_480px"
            app:block_poster_holder="@drawable/focus_528_480"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_029_1">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_view1"
            app:block_poster_height="@dimen/height_162px"
            app:block_poster_holder="@drawable/focus_240_162"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_029_6">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_view1"
            android:layout_toEndOf="@id/id_module_29_view2"
            android:layout_toRightOf="@id/id_module_29_view2"
            app:block_poster_height="@dimen/height_162px"
            app:block_poster_holder="@drawable/focus_240_162"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_029_7">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>


        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_title"
            android:layout_toEndOf="@id/id_module_29_view1"
            android:layout_toRightOf="@id/id_module_29_view1"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_528_216"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_029_2">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>


        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_view4"
            android:layout_toEndOf="@id/id_module_29_view1"
            android:layout_toRightOf="@id/id_module_29_view1"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_528_216"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_029_4">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_view1"
            android:layout_toEndOf="@id/id_module_29_view3"
            android:layout_toRightOf="@id/id_module_29_view3"
            app:block_poster_height="@dimen/height_162px"
            app:block_poster_holder="@drawable/focus_240_162"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_029_8">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_view1"
            android:layout_toEndOf="@id/id_module_29_view6"
            android:layout_toRightOf="@id/id_module_29_view6"
            app:block_poster_height="@dimen/height_162px"
            app:block_poster_holder="@drawable/focus_240_162"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_029_9">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_title"
            android:layout_toEndOf="@id/id_module_29_view4"
            android:layout_toRightOf="@id/id_module_29_view4"
            app:block_last="true"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_528_216"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_029_3">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>


        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_view4"
            android:layout_toEndOf="@id/id_module_29_view5"
            android:layout_toRightOf="@id/id_module_29_view5"
            app:block_last="true"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_528_216"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_029_5">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_view1"
            android:layout_toEndOf="@id/id_module_29_view7"
            android:layout_toRightOf="@id/id_module_29_view7"
            app:block_poster_height="@dimen/height_162px"
            app:block_poster_holder="@drawable/focus_240_162"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_029_10">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_29_view11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_29_view1"
            android:layout_toEndOf="@id/id_module_29_view10"
            android:layout_toRightOf="@id/id_module_29_view10"
            app:block_last="true"
            app:block_poster_height="@dimen/height_162px"
            app:block_poster_holder="@drawable/focus_240_162"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_029_11">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

    </RelativeLayout>
</RelativeLayout>





