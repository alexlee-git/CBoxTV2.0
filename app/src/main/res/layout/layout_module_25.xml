<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <tv.newtv.cboxtv.views.custom.RecycleImageView
        android:id="@+id/id_module_25_title_icon"
        android:layout_width="@dimen/width_17px"
        android:layout_height="@dimen/height_17px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_68px"
        android:background="@drawable/tuoyuan"
        android:tag="module_025_title_icon"
        android:visibility="gone" />

    <TextView
        android:id="@+id/id_module_25_title"
        style="@style/ModuleTitleStyle"
        android:layout_width="@dimen/width_1830px"
        android:layout_height="@dimen/height_34px"
        android:layout_marginLeft="@dimen/width_8px"
        android:layout_marginTop="@dimen/height_59px"
        android:layout_toRightOf="@id/id_module_25_title_icon"
        android:includeFontPadding="false"
        android:tag="module_025_title"
        android:visibility="gone" />
    <!-- 1 -->
    <FrameLayout
        android:id="@+id/id_module_25_view1"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_228px"
        android:layout_below="@id/id_module_25_title"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_025_1">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_240px"
            android:layout_height="@dimen/height_162px"
            android:tag="cell_025_1_poster" />

    </FrameLayout>

    <!-- 2 -->
    <FrameLayout
        android:id="@+id/id_module_25_view2"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_228px"
        android:layout_below="@id/id_module_25_title"
        android:layout_marginLeft="@dimen/module_25_9px"
        android:layout_marginTop="@dimen/height_15px"
        android:layout_toRightOf="@id/id_module_25_view1"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_025_2">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_240px"
            android:layout_height="@dimen/height_162px"
            android:tag="cell_025_2_poster" />

    </FrameLayout>

    <!-- 3 -->
    <FrameLayout
        android:id="@+id/id_module_25_view3"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_228px"
        android:layout_below="@id/id_module_25_title"
        android:layout_marginLeft="@dimen/module_25_9px"
        android:layout_marginTop="@dimen/height_15px"
        android:layout_toRightOf="@id/id_module_25_view2"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_025_3">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_240px"
            android:layout_height="@dimen/height_162px"
            android:tag="cell_025_3_poster" />

    </FrameLayout>

    <!-- 4 -->
    <FrameLayout
        android:id="@+id/id_module_25_view4"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_228px"
        android:layout_below="@id/id_module_25_title"
        android:layout_marginLeft="@dimen/module_25_9px"
        android:layout_marginTop="@dimen/height_15px"
        android:layout_toRightOf="@id/id_module_25_view3"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_025_4">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_240px"
            android:layout_height="@dimen/height_162px"
            android:tag="cell_025_4_poster" />

    </FrameLayout>

    <!-- 5 -->
    <FrameLayout
        android:id="@+id/id_module_25_view5"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_228px"
        android:layout_below="@id/id_module_25_title"
        android:layout_marginLeft="@dimen/module_25_9px"
        android:layout_marginTop="@dimen/height_15px"
        android:layout_toRightOf="@id/id_module_25_view4"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_025_5">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_240px"
            android:layout_height="@dimen/height_162px"
            android:tag="cell_025_5_poster" />

    </FrameLayout>

    <!-- 6 -->
    <FrameLayout
        android:id="@+id/id_module_25_view6"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_228px"
        android:layout_below="@id/id_module_25_title"
        android:layout_marginLeft="@dimen/module_25_9px"
        android:layout_marginTop="@dimen/height_15px"
        android:layout_toRightOf="@id/id_module_25_view5"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_025_6">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_240px"
            android:layout_height="@dimen/height_162px"
            android:tag="cell_025_6_poster" />

    </FrameLayout>

</RelativeLayout>


