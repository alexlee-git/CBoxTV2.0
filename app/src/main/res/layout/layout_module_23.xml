<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <!--两个-->
    <!--<TextView-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="test23"-->
        <!--android:textColor="#fff" />-->

    <tv.newtv.cboxtv.views.custom.RecycleImageView
        android:id="@+id/id_module_23_title_icon"
        android:layout_width="@dimen/width_17px"
        android:layout_height="@dimen/height_17px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_68px"
        android:background="@drawable/tuoyuan"
        android:tag="module_023_title_icon"
        android:visibility="gone" />

    <TextView
        android:id="@+id/id_module_23_title"
        style="@style/ModuleTitleStyle"
        android:layout_width="@dimen/width_376px"
        android:layout_height="@dimen/height_34px"
        android:layout_marginLeft="@dimen/width_8px"
        android:layout_marginTop="@dimen/height_59px"
        android:layout_toRightOf="@id/id_module_23_title_icon"
        android:includeFontPadding="false"
        android:tag="module_023_title"
        android:visibility="gone" />

    <FrameLayout
        android:id="@+id/id_module_23_view1"
        android:layout_width="@dimen/width_882px"
        android:layout_height="@dimen/height_306px"
        android:layout_below="@id/id_module_23_title"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_023_1">

        <!--海报-->
        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_816px"
            android:layout_height="@dimen/height_240px"
            android:tag="cell_023_1_poster" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/id_module_23_view2"
        android:layout_width="@dimen/width_882px"
        android:layout_height="@dimen/height_306px"
        android:layout_below="@id/id_module_23_title"
        android:layout_marginLeft="-18px"
        android:layout_marginTop="@dimen/height_15px"
        android:layout_toRightOf="@id/id_module_23_view1"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_023_2">

        <!--海报-->
        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_816px"
            android:layout_height="@dimen/height_240px"
            android:tag="cell_023_2_poster" />

    </FrameLayout>


</RelativeLayout>


