<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <tv.newtv.cboxtv.views.custom.RecycleImageView
        android:id="@+id/id_module_22_title_icon"
        android:layout_width="@dimen/width_17px"
        android:layout_height="@dimen/height_17px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_56px"
        android:background="@drawable/tuoyuan"
        android:tag="module_022_title_icon"
        android:visibility="gone" />

    <TextView
        android:id="@+id/id_module_22_title"
        style="@style/ModuleTitleStyle"
        android:layout_width="@dimen/width_376px"
        android:layout_height="@dimen/height_34px"
        android:layout_marginLeft="@dimen/width_6px"
        android:layout_marginTop="@dimen/height_47px"
        android:layout_toRightOf="@id/id_module_22_title_icon"
        android:includeFontPadding="false"
        android:tag="module_022_title"
        android:visibility="gone" />

    <!-- 1 -->
    <FrameLayout
        android:id="@+id/id_module_22_view1"
        android:layout_width="@dimen/width_882px"
        android:layout_height="@dimen/height_546px"
        android:layout_below="@id/id_module_22_title"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_022_1">

        <tv.newtv.cboxtv.views.custom.LivePlayView
            android:layout_width="@dimen/width_816px"
            android:layout_height="@dimen/height_480px"
            android:tag="cell_022_1_poster">
            <tv.newtv.cboxtv.views.custom.RecycleImageView
                android:scaleType="fitXY"
                android:layout_width="@dimen/width_816px"
                android:layout_height="@dimen/height_480px"/>
        </tv.newtv.cboxtv.views.custom.LivePlayView>

    </FrameLayout>

    <!-- 2 -->
    <FrameLayout
        android:id="@+id/id_module_22_view2"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_22_view1"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="-18px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_022_6">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_240px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_022_6_poster" />

    </FrameLayout>

    <!-- 3 -->
    <FrameLayout
        android:id="@+id/id_module_22_view3"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_22_view1"
        android:layout_marginLeft="@dimen/_width_11px"
        android:layout_marginTop="-18px"
        android:layout_toRightOf="@id/id_module_22_view2"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_022_7">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_240px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_022_7_poster" />

    </FrameLayout>

    <!-- 4 -->
    <FrameLayout
        android:id="@+id/id_module_22_view4"
        android:layout_width="@dimen/width_306px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_22_view1"
        android:layout_marginLeft="@dimen/_width_11px"
        android:layout_marginTop="-18px"
        android:layout_toRightOf="@id/id_module_22_view3"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_022_8">

