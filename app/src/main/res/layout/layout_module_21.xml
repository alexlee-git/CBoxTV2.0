        android:layout_width="@dimen/width_594px"
        android:layout_height="@dimen/height_362px"
        android:layout_below="@id/id_module_21_title"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_021_1">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_528px"
            android:layout_height="@dimen/height_296px"
            android:scaleType="fitXY"
            android:tag="cell_021_1_poster" />

        <!-- 左上角角标 -->
        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_51px"
            android:layout_height="@dimen/height_29px"
            android:layout_marginLeft="@dimen/width_45px"
            android:layout_marginTop="@dimen/height_45px" />

    </FrameLayout>
