        android:layout_marginLeft="@dimen/width_404px"
        android:layout_marginTop="@dimen/height_478px"
        android:background="@drawable/selector_keyboard_number"
        android:clickable="true"
        android:focusable="true">

        <Button
            android:id="@+id/btn_6"
            style="@style/StyleSearchTitle"
            android:layout_width="@dimen/width_130px"
            android:layout_height="@dimen/height_130px"
            android:background="@color/color"
            android:focusable="false"
            android:gravity="center"
            android:text="6"
            />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout_btn_7"
        android:layout_width="@dimen/width_130px"
        android:layout_height="@dimen/height_130px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_620px"
        android:background="@drawable/selector_keyboard_number"
        android:clickable="true"
        android:focusable="true">

        <Button
            android:id="@+id/btn_7"
            style="@style/StyleSearchTitle"
            android:layout_width="@dimen/width_130px"
            android:layout_height="@dimen/height_130px"
            android:background="@color/color"
            android:focusable="false"
            android:gravity="center"
            android:text="7"
            />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout_btn_8"
        android:layout_width="@dimen/width_130px"
        android:layout_height="@dimen/height_130px"
        android:layout_marginLeft="@dimen/width_262px"
        android:layout_marginTop="@dimen/height_620px"
        android:background="@drawable/selector_keyboard_number"
        android:clickable="true"
        android:focusable="true">

        <Button
            android:id="@+id/btn_8"
            style="@style/StyleSearchTitle"
            android:layout_width="@dimen/width_130px"
            android:layout_height="@dimen/height_130px"
            android:background="@color/color"
            android:focusable="false"
            android:gravity="center"
            android:text="8"
            />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout_btn_9"
        android:layout_width="@dimen/width_130px"
        android:layout_height="@dimen/height_130px"
        android:layout_marginLeft="@dimen/width_404px"
        android:layout_marginTop="@dimen/height_620px"
        android:background="@drawable/selector_keyboard_number"
        android:clickable="true"
        android:focusable="true">

        <Button
            android:id="@+id/btn_9"
            style="@style/StyleSearchTitle"
            android:layout_width="@dimen/width_130px"
            android:layout_height="@dimen/height_130px"
            android:background="@color/color"
            android:focusable="false"
            android:gravity="center"
            android:text="9"
            />
    </FrameLayout>


    <FrameLayout
        android:id="@+id/frameLayout_btn_11"
        android:layout_width="@dimen/width_201px"
        android:layout_height="@dimen/height_80px"
        android:layout_marginLeft="@dimen/width_333px"
        android:layout_marginTop="@dimen/height_244px"
        android:background="@drawable/selector_keyboard_del"
        android:clickable="true"
        android:focusable="true">

        <ImageView
            android:layout_width="@dimen/width_39px"
