        android:layout_marginTop="@dimen/height_244px"
        android:background="@drawable/selector_keyboard_del"
        android:clickable="true"
        android:focusable="true">

        <ImageView
            android:layout_width="@dimen/width_27px"
            android:layout_height="@dimen/height_27px"
            android:layout_marginLeft="@dimen/width_43px"
            android:layout_marginTop="@dimen/height_27px"
            android:background="@drawable/search_dustbin"
            android:focusable="false"/>

        <Button
            android:id="@+id/btn_10"
            style="@style/StyleSearchTitle"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_80px"
            android:layout_marginLeft="@dimen/width_99px"
            android:layout_marginTop="0px"
            android:background="@color/color"
            android:clickable="true"
            android:focusable="false"
            android:gravity="center|left"
            android:text="清空"/>

    </FrameLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_64px"
        android:layout_marginBottom="@dimen/height_50px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_966px"
        android:gravity="center|left"
        android:text="@string/search_keybroad_show"
        android:textColor="#ededed"
        android:textSize="@dimen/height_27px"
        />



</RelativeLayout>