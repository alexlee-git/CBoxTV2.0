<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/keyboard"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
    >

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/search_keyboard_bg"
        android:focusable="false"/>

    <LinearLayout
        android:layout_width="@dimen/width_414px"
        android:layout_height="@dimen/height_80px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_66px"
        android:background="@drawable/search_title_bg"
        android:focusable="false"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="@dimen/width_30px"
            android:layout_height="@dimen/height_30px"
            android:layout_marginLeft="@dimen/width_29px"
            android:background="@drawable/search_icon"/>

        <TextView
            android:id="@+id/txt_input"
            style="@style/StyleSearchKeyBoardTitle"
            android:layout_width="@dimen/width_324px"
            android:layout_height="@dimen/height_80px"
            android:layout_marginLeft="@dimen/width_31px"
            android:gravity="center|left"
            android:hint="@string/search_keybroad_input"
            android:maxLength="10"/>
    </LinearLayout>


    <FrameLayout
        android:id="@+id/frameLayout_btn_1"
        android:layout_width="@dimen/width_130px"
        android:layout_height="@dimen/height_130px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_336px"
        android:background="@drawable/selector_keyboard_number"
        android:clickable="true"
        android:focusable="true"
        >

        <ImageView
            android:layout_width="@dimen/width_68px"
            android:layout_height="@dimen/height_19px"
            android:layout_marginLeft="@dimen/width_33px"
            android:layout_marginTop="@dimen/height_85px"
            android:background="@drawable/search_keyboard_space"/>

        <Button
            android:id="@+id/btn_1"
            style="@style/StyleSearchTitle"
            android:layout_width="@dimen/width_51px"
            android:layout_height="@dimen/height_40px"
            android:layout_marginLeft="@dimen/width_41px"
            android:layout_marginTop="@dimen/height_30px"
            android:background="@color/color"
            android:text="0|1"/>


    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout_btn_2"
        android:layout_width="@dimen/width_130px"
        android:layout_height="@dimen/height_130px"
        android:layout_marginLeft="@dimen/width_262px"
        android:layout_marginTop="@dimen/height_336px"
        android:background="@drawable/selector_keyboard_number"
        android:clickable="true"
        android:focusable="true">

        <Button
            android:id="@+id/btn_2"
            style="@style/StyleSearchTitle"
            android:layout_width="@dimen/width_130px"
            android:layout_height="@dimen/height_130px"
            android:background="@color/color"
            android:focusable="false"
            android:gravity="center"
            android:text="2"
            />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout_btn_3"
        android:layout_width="@dimen/width_130px"
        android:layout_height="@dimen/height_130px"
        android:layout_marginLeft="@dimen/width_404px"
        android:layout_marginTop="@dimen/height_336px"
        android:background="@drawable/selector_keyboard_number"
        android:clickable="true"
        android:focusable="true">

        <Button
            android:id="@+id/btn_3"
            style="@style/StyleSearchTitle"
            android:layout_width="@dimen/width_130px"
            android:layout_height="@dimen/height_130px"
            android:background="@color/color"
            android:focusable="false"
            android:gravity="center"
            android:text="3"
            />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout_btn_5"
        android:layout_width="@dimen/width_130px"
        android:layout_height="@dimen/height_130px"
        android:layout_marginLeft="@dimen/width_262px"
        android:layout_marginTop="@dimen/height_478px"
        android:background="@drawable/selector_keyboard_number"
        android:clickable="true"
        android:focusable="true">

        <Button
            android:id="@+id/btn_5"
            style="@style/StyleSearchTitle"
            android:layout_width="@dimen/width_130px"
            android:layout_height="@dimen/height_130px"
            android:background="@color/color"
            android:focusable="false"
            android:gravity="center"
            android:text="5"
