<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">

    <tv.newtv.cboxtv.views.custom.RecycleImageView
        android:id="@+id/id_module_22_title_icon"
        android:layout_width="@dimen/width_17px"
        android:layout_height="@dimen/height_17px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_56px"
        android:background="@drawable/tuoyuan"
        android:tag="module_022_title_icon"
        android:visibility="visible" />

    <TextView
        android:id="@+id/id_module_22_title"
        style="@style/ModuleTitleStyle"
        android:layout_width="@dimen/width_1830px"
        android:layout_height="@dimen/height_34px"
        android:layout_marginLeft="@dimen/width_6px"
        android:layout_marginTop="@dimen/height_47px"
        android:layout_toRightOf="@id/id_module_22_title_icon"
        android:includeFontPadding="false"
        android:tag="module_022_title"
        android:visibility="visible" />

    <!-- 1 -->
    <tv.newtv.cboxtv.views.custom.BlockPosterView
        android:id="@+id/id_module_22_view1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/id_module_22_title"
        app:block_poster_width="@dimen/width_816px"
        app:block_poster_height="@dimen/height_480px"
        app:block_poster_holder="@drawable/focus_816_480"
        app:block_poster_type="video"
        app:block_tag="cell_022_1">

    </tv.newtv.cboxtv.views.custom.BlockPosterView>

    <!-- 2 -->
    <tv.newtv.cboxtv.views.custom.BlockPosterView
        android:id="@+id/id_module_22_view2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/id_module_22_view1"
        app:block_poster_width="@dimen/width_240px"
        app:block_poster_height="@dimen/height_216px"
        app:block_poster_holder="@drawable/focus_240_216"
        app:block_tag="cell_022_6">

    </tv.newtv.cboxtv.views.custom.BlockPosterView>

    <!-- 3 -->
    <tv.newtv.cboxtv.views.custom.BlockPosterView
        android:id="@+id/id_module_22_view3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/id_module_22_view1"
        app:block_poster_width="@dimen/width_240px"
        app:block_poster_height="@dimen/height_216px"
        app:block_poster_holder="@drawable/focus_240_216"
        android:layout_toRightOf="@id/id_module_22_view2"
        app:block_tag="cell_022_7"
        android:layout_toEndOf="@id/id_module_22_view2">

    </tv.newtv.cboxtv.views.custom.BlockPosterView>

    <!-- 4 -->
    <tv.newtv.cboxtv.views.custom.BlockPosterView
        android:id="@+id/id_module_22_view4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/id_module_22_view1"
        app:block_poster_width="@dimen/width_240px"
        app:block_poster_height="@dimen/height_216px"
        app:block_poster_holder="@drawable/focus_240_216"
        android:layout_toRightOf="@id/id_module_22_view3"
        app:block_tag="cell_022_8"
        android:layout_toEndOf="@id/id_module_22_view3">

    </tv.newtv.cboxtv.views.custom.BlockPosterView>

    <!-- 5 -->
    <tv.newtv.cboxtv.views.custom.BlockPosterView
        android:id="@+id/id_module_22_view5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/id_module_22_title"
        app:block_poster_width="@dimen/width_384px"
        app:block_poster_height="@dimen/height_216px"
        app:block_poster_holder="@drawable/focus_384_216"
        android:layout_toRightOf="@id/id_module_22_view1"
        app:block_tag="cell_022_2"
        android:layout_toEndOf="@id/id_module_22_view1">

    </tv.newtv.cboxtv.views.custom.BlockPosterView>

    <!-- 6 -->
    <tv.newtv.cboxtv.views.custom.BlockPosterView
        android:id="@+id/id_module_22_view6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/id_module_22_view5"
        app:block_poster_width="@dimen/width_384px"
        app:block_poster_height="@dimen/height_216px"
        app:block_poster_holder="@drawable/focus_384_216"
        android:layout_toRightOf="@id/id_module_22_view1"
        app:block_tag="cell_022_4"
        android:layout_toEndOf="@id/id_module_22_view1">

    </tv.newtv.cboxtv.views.custom.BlockPosterView>

    <!-- 7 -->
    <tv.newtv.cboxtv.views.custom.BlockPosterView
        android:id="@+id/id_module_22_view7"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_22_view6"
        android:layout_marginLeft="@dimen/_width_18px"
        android:layout_marginTop="@dimen/_width_7px"
        android:layout_toRightOf="@id/id_module_22_view4"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_022_9">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_022_9_poster" />

    </tv.newtv.cboxtv.views.custom.BlockPosterView>

    <!-- 8 -->
    <FrameLayout
        android:id="@+id/id_module_22_view8"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_22_title"
        android:layout_marginLeft="@dimen/_width_18px"
        android:layout_marginTop="@dimen/height_15px"
        android:layout_toRightOf="@id/id_module_22_view5"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_022_3">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_022_3_poster" />

    </FrameLayout>

    <!-- 9 -->
    <FrameLayout
        android:id="@+id/id_module_22_view9"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_22_view8"
        android:layout_marginLeft="@dimen/_width_18px"
        android:layout_marginTop="@dimen/_width_7px"
        android:layout_toRightOf="@id/id_module_22_view6"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_022_5">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_022_5_poster" />

    </FrameLayout>

    <!-- 10 -->
    <FrameLayout
        android:id="@+id/id_module_22_view10"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_22_view9"
        android:layout_marginLeft="@dimen/_width_18px"
        android:layout_marginTop="@dimen/_width_7px"
        android:layout_toRightOf="@id/id_module_22_view7"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_022_10">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_022_10_poster" />

    </FrameLayout>

</RelativeLayout>
</RelativeLayout>

