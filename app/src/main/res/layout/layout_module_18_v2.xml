<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/height_15px"
    android:paddingBottom="@dimen/height_15px">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">

        <ImageView
            android:id="@+id/id_module_18_title_icon"
            android:layout_width="@dimen/width_17px"
            android:layout_height="@dimen/height_17px"
            android:layout_marginLeft="@dimen/width_20px"
            android:layout_marginTop="@dimen/height_56px"
            android:background="@drawable/tuoyuan"
            android:tag="module_018_title_icon"
            android:visibility="gone" />

        <TextView
            android:id="@+id/id_module_18_title"
            style="@style/ModuleTitleStyle"
            android:layout_width="@dimen/width_1600px"
            android:layout_height="@dimen/height_34px"
            android:layout_marginLeft="@dimen/width_6px"
            android:layout_marginTop="@dimen/height_47px"
            android:layout_toRightOf="@id/id_module_18_title_icon"
            android:includeFontPadding="false"
            android:tag="module_018_title"
            android:visibility="gone" />

        <!-- 1 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:layout_marginTop="@dimen/height_15px"
            android:id="@+id/id_module_18_view1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_18_title"
            app:block_poster_height="@dimen/height_480px"
            app:block_poster_holder="@drawable/focus_816_480"
            app:block_poster_type="video"
            app:block_poster_width="@dimen/width_816px"
            app:block_tag="cell_018_1">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 2 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_18_view2"
            android:layout_width="@dimen/width_882px"
            android:layout_height="@dimen/height_282px"
            android:layout_below="@id/id_module_18_view1"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_816_216"
            app:block_poster_width="@dimen/width_816px"
            app:block_tag="cell_018_4">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 3 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:layout_marginTop="@dimen/height_15px"
            android:id="@+id/id_module_18_view3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_18_title"
            android:layout_toEndOf="@id/id_module_18_view1"
            android:layout_toRightOf="@id/id_module_18_view1"
            app:block_poster_height="@dimen/height_480px"
            app:block_poster_holder="@drawable/focus_384_480"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_018_2">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 4 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_18_view4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_18_view3"
            android:layout_toEndOf="@id/id_module_18_view2"
            android:layout_toRightOf="@id/id_module_18_view2"
            app:block_last="true"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_816_216"
            app:block_poster_width="@dimen/width_816px"
            app:block_tag="cell_018_5">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 5 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:layout_marginTop="@dimen/height_15px"
            android:id="@+id/id_module_18_view5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_18_title"
            android:layout_toEndOf="@id/id_module_18_view3"
            android:layout_toRightOf="@id/id_module_18_view3"
            app:block_last="true"
            app:block_poster_height="@dimen/height_480px"
            app:block_poster_holder="@drawable/focus_384_480"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_018_3">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

    </RelativeLayout>
</RelativeLayout>

