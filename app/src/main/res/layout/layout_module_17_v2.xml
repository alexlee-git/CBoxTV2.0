<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clipToPadding="false"
                android:paddingTop="@dimen/height_15px"
                android:paddingBottom="@dimen/height_15px">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:id="@+id/id_module_17_title_icon"
            android:layout_width="@dimen/width_17px"
            android:layout_height="@dimen/height_17px"
            android:layout_marginStart="@dimen/width_20px"
            android:layout_marginLeft="@dimen/width_120px"
            android:layout_marginTop="@dimen/height_68px"
            android:background="@drawable/tuoyuan"
            android:tag="module_017_title_icon"
            android:visibility="gone"/>

        <TextView
            android:id="@+id/id_module_17_title"
            style="@style/ModuleTitleStyle"
            android:layout_width="@dimen/width_1600px"
            android:layout_height="@dimen/height_34px"
            android:layout_marginStart="@dimen/width_8px"
            android:layout_marginLeft="@dimen/width_8px"
            android:layout_marginTop="@dimen/height_59px"
            android:layout_toEndOf="@id/id_module_17_title_icon"
            android:layout_toRightOf="@id/id_module_17_title_icon"
            android:includeFontPadding="false"
            android:tag="module_017_title"
            android:visibility="gone"/>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:layout_below="@+id/id_module_17_title"
            android:id="@id/id_module_17_view1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:block_poster_height="@dimen/height_480px"
            app:block_poster_holder="@drawable/focus_816_480"
            app:block_poster_type="video"
            app:block_poster_width="@dimen/width_816px"
            app:block_tag="cell_017_1">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_17_view2"
            android:layout_below="@+id/id_module_17_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/id_module_17_view1"
            android:layout_toRightOf="@+id/id_module_17_view1"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_017_2">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>


        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:layout_below="@+id/id_module_17_title"
            android:id="@+id/id_module_17_view3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/id_module_17_view2"
            android:layout_toRightOf="@+id/id_module_17_view2"
            app:block_poster_height="@dimen/height_216px"
            app:block_last="true"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_017_3">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_17_view4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/id_module_17_view2"
            android:layout_toEndOf="@+id/id_module_17_view1"
            android:layout_toRightOf="@+id/id_module_17_view1"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_017_4">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_17_view5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/id_module_17_view3"
            android:layout_toEndOf="@+id/id_module_17_view4"
            android:layout_toRightOf="@+id/id_module_17_view4"
            app:block_poster_height="@dimen/height_216px"
            app:block_last="true"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_017_5">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_17_view6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/id_module_17_view1"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_017_6">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_17_view7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/id_module_17_view1"
            android:layout_toEndOf="@+id/id_module_17_view6"
            android:layout_toRightOf="@+id/id_module_17_view6"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_017_7">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_17_view8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/id_module_17_view1"
            android:layout_toEndOf="@+id/id_module_17_view7"
            android:layout_toRightOf="@+id/id_module_17_view7"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_017_8">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_17_view9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:block_last="true"
            android:layout_below="@+id/id_module_17_view1"
            android:layout_toEndOf="@+id/id_module_17_view8"
            android:layout_toRightOf="@+id/id_module_17_view8"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_017_9">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

    </RelativeLayout>
</RelativeLayout>


