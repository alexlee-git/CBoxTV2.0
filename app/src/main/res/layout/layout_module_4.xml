<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <tv.newtv.cboxtv.views.custom.RecycleImageView
        android:id="@+id/id_module_4_title_icon"
        android:layout_width="@dimen/width_17px"
        android:layout_height="@dimen/height_17px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_56px"
        android:background="@drawable/tuoyuan"
        android:tag="module_004_title_icon"
        android:visibility="gone" />

    <TextView
        android:id="@+id/id_module_4_title"
        style="@style/ModuleTitleStyle"
        android:layout_width="@dimen/width_1827px"
        android:layout_height="@dimen/height_34px"
        android:layout_marginLeft="@dimen/width_6px"
        android:layout_marginTop="@dimen/height_47px"
        android:layout_toRightOf="@id/id_module_4_title_icon"
        android:includeFontPadding="false"
        android:tag="module_004_title"
        android:visibility="gone" />

    <FrameLayout
        android:id="@+id/id_module_4_view1"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@+id/id_module_4_title"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_004_1">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_004_1_poster" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/id_module_4_view2"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_4_title"
        android:layout_marginLeft="@dimen/width_519px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_004_2">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_004_2_poster" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/id_module_4_view3"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_4_title"
        android:layout_marginLeft="@dimen/width_951px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_004_3">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_004_3_poster" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/id_module_4_view4"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_4_title"
        android:layout_marginLeft="@dimen/width_1383px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_004_4">

        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:tag="cell_004_4_poster" />

    </FrameLayout>

</RelativeLayout>


