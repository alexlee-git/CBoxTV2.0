<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">

        <ImageView
            android:id="@+id/id_module_4_title_icon"
            android:layout_width="@dimen/width_17px"
            android:layout_height="@dimen/height_17px"
            android:layout_marginLeft="@dimen/width_20px"
            android:layout_marginStart="@dimen/width_20px"
            android:layout_marginTop="@dimen/height_56px"
            android:background="@drawable/tuoyuan"
            android:tag="module_004_title_icon"
            android:visibility="gone" />

        <TextView
            android:id="@+id/id_module_4_title"
            style="@style/ModuleTitleStyle"
            android:layout_width="@dimen/width_1600px"
            android:layout_height="@dimen/height_34px"
            android:layout_marginLeft="@dimen/width_6px"
            android:layout_marginTop="@dimen/height_47px"
            android:layout_toRightOf="@id/id_module_4_title_icon"
            android:includeFontPadding="false"
            android:tag="module_004_title"
            android:visibility="gone" />

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_4_view1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/height_15px"
            android:layout_below="@+id/id_module_4_title"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_004_1">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_4_view2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/height_15px"
            android:layout_below="@id/id_module_4_title"
            android:layout_toEndOf="@id/id_module_4_view1"
            android:layout_toRightOf="@id/id_module_4_view1"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_004_2">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_4_view3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/height_15px"
            android:layout_below="@id/id_module_4_title"
            android:layout_toEndOf="@id/id_module_4_view2"
            android:layout_toRightOf="@id/id_module_4_view2"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_004_3">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_4_view4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/height_15px"
            android:layout_below="@id/id_module_4_title"
            android:layout_toRightOf="@id/id_module_4_view3"
            app:block_last="true"
            app:block_poster_height="@dimen/height_216px"
            app:block_poster_holder="@drawable/focus_384_216"
            app:block_poster_width="@dimen/width_384px"
            app:block_tag="cell_004_4"
            android:layout_toEndOf="@id/id_module_4_view3">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

    </RelativeLayout>
</RelativeLayout>


