<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <tv.newtv.cboxtv.views.custom.RecycleImageView
        android:id="@+id/id_module_9_title_icon"
        android:layout_width="@dimen/width_17px"
        android:layout_height="@dimen/height_17px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_68px"
        android:background="@drawable/tuoyuan"
        android:tag="module_009_title_icon"
        android:visibility="gone" />

    <TextView
        android:id="@+id/id_module_9_title"
        style="@style/ModuleTitleStyle"
        android:layout_width="@dimen/width_376px"
        android:layout_height="@dimen/height_34px"
        android:layout_marginLeft="@dimen/width_8px"
        android:layout_marginTop="@dimen/height_59px"
        android:layout_toRightOf="@id/id_module_9_title_icon"
        android:includeFontPadding="false"
        android:tag="module_009_title"
        android:visibility="gone" />

    <FrameLayout
        android:id="@+id/id_module_9_view1"
        android:layout_width="@dimen/width_882px"
        android:layout_height="@dimen/height_546px"
        android:layout_below="@id/id_module_9_title"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_009_1">

        <!--海报-->
        <tv.newtv.cboxtv.views.custom.LivePlayView
            android:layout_width="@dimen/width_816px"
            android:layout_height="@dimen/height_480px"
            android:tag="cell_009_1_poster">
            <tv.newtv.cboxtv.views.custom.RecycleImageView
                android:scaleType="fitXY"
                android:layout_width="@dimen/width_816px"
                android:layout_height="@dimen/height_480px"/>
        </tv.newtv.cboxtv.views.custom.LivePlayView>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/id_module_9_view2"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_below="@id/id_module_9_title"
        android:layout_marginLeft="@dimen/width_960px"
        android:layout_alignTop="@+id/id_module_9_view1"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_009_2">

        <!--海报-->
        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:scaleType="fitXY"
            android:tag="cell_009_2_poster" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/id_module_9_view3"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_alignBottom="@+id/id_module_9_view1"
        android:layout_marginLeft="@dimen/width_960px"
        android:layout_marginTop="@dimen/height_279px"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_009_4">

        <!--海报-->
        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:scaleType="fitXY"
            android:tag="cell_009_4_poster" />

    </FrameLayout>

    <FrameLayout
        android:layout_marginLeft="@dimen/width_1393px"
        android:id="@+id/id_module_9_view4"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_alignTop="@+id/id_module_9_view1"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_009_3">

        <!--海报-->
        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:scaleType="fitXY"
            android:tag="cell_009_3_poster" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/id_module_9_view5"
        android:layout_width="@dimen/width_450px"
        android:layout_height="@dimen/height_282px"
        android:layout_marginLeft="@dimen/width_1395px"
        android:layout_marginTop="@dimen/height_279px"
        android:layout_alignBottom="@+id/id_module_9_view1"
        android:background="@drawable/selector_pos_background_27px"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_009_5">

        <!--海报-->
        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:layout_width="@dimen/width_384px"
            android:layout_height="@dimen/height_216px"
            android:scaleType="fitXY"
            android:tag="cell_009_5_poster" />

    </FrameLayout>
</RelativeLayout>


