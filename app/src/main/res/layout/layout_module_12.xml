<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <tv.newtv.cboxtv.views.RecycleImageView
        android:id="@+id/id_module_12_title_icon"
        android:layout_width="@dimen/width_17px"
        android:layout_height="@dimen/height_17px"
        android:layout_marginLeft="@dimen/width_120px"
        android:layout_marginTop="@dimen/height_56px"
        android:background="@drawable/tuoyuan"
        android:tag="module_012_title_icon"
        android:visibility="gone" />

    <TextView
        android:id="@+id/id_module_12_title"
        style="@style/ModuleTitleStyle"
        android:layout_width="@dimen/width_374px"
        android:layout_height="@dimen/height_34px"
        android:layout_marginLeft="@dimen/width_6px"
        android:layout_marginTop="@dimen/height_47px"
        android:layout_toRightOf="@id/id_module_12_title_icon"
        android:includeFontPadding="false"
        android:tag="module_012_title"
        android:visibility="gone" />


    <!-- 1 -->
    <FrameLayout
        android:id="@+id/id_module_12_view1"
        android:layout_width="@dimen/width_882px"
        android:layout_height="@dimen/height_386px"
        android:layout_below="@id/id_module_12_title"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_012_1">

        <tv.newtv.cboxtv.views.RecycleImageView
            android:layout_width="@dimen/width_816px"
            android:layout_height="@dimen/height_320px"
            android:tag="cell_012_1_poster" />

    </FrameLayout>

    <!-- 2 -->
    <FrameLayout
        android:id="@+id/id_module_12_view2"
        android:layout_width="@dimen/width_594px"
        android:layout_height="@dimen/height_362px"
        android:layout_below="@id/id_module_12_view1"
        android:layout_marginLeft="@dimen/width_87px"
        android:layout_marginTop="@dimen/_height_18px"
        android:background="@drawable/selector_pos_background"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_012_3">

        <tv.newtv.cboxtv.views.RecycleImageView
            android:layout_width="@dimen/width_528px"
            android:layout_height="@dimen/height_296px"
            android:tag="cell_012_3_poster" />

    </FrameLayout>

    <!-- 3 -->
    <FrameLayout
        android:id="@+id/id_module_12_view3"
        android:layout_width="@dimen/width_594px"
        android:layout_height="@dimen/height_362px"
        android:layout_below="@id/id_module_12_view1"
        android:layout_marginLeft="@dimen/width_664px"
        android:layout_marginTop="@dimen/_height_18px"
        android:background="@drawable/selector_pos_background"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_012_4">

        <tv.newtv.cboxtv.views.RecycleImageView
            android:layout_width="@dimen/width_528px"
            android:layout_height="@dimen/height_296px"
            android:tag="cell_012_4_poster" />

    </FrameLayout>

    <!-- 4 -->
    <FrameLayout
        android:id="@+id/id_module_12_view4"
        android:layout_width="@dimen/width_882px"
        android:layout_height="@dimen/height_386px"
        android:layout_below="@id/id_module_12_title"
        android:layout_marginLeft="@dimen/width_951px"
        android:layout_marginTop="@dimen/height_15px"
        android:background="@drawable/selector_pos_background"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_012_2">

        <tv.newtv.cboxtv.views.RecycleImageView
            android:layout_width="@dimen/width_816px"
            android:layout_height="@dimen/height_320px"
            android:tag="cell_012_2_poster" />

    </FrameLayout>

    <!-- 5 -->
    <FrameLayout
        android:id="@+id/id_module_12_view5"
        android:layout_width="@dimen/width_594px"
        android:layout_height="@dimen/height_362px"
        android:layout_below="@id/id_module_12_view4"
        android:layout_marginLeft="@dimen/width_1240px"
        android:layout_marginTop="@dimen/_height_18px"
        android:background="@drawable/selector_pos_background"
        android:clickable="true"
        android:focusable="true"
        android:tag="cell_012_5">

        <tv.newtv.cboxtv.views.RecycleImageView
            android:layout_width="@dimen/width_528px"
            android:layout_height="@dimen/height_296px"
            android:tag="cell_012_5_poster" />

    </FrameLayout>

</RelativeLayout>


