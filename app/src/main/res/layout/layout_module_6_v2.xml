<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">
        <!--八个-->
        <tv.newtv.cboxtv.views.custom.RecycleImageView
            android:id="@+id/id_module_6_title_icon"
            android:layout_width="@dimen/width_17px"
            android:layout_height="@dimen/height_17px"
            android:layout_marginLeft="@dimen/width_20px"
            android:layout_marginTop="@dimen/height_56px"
            android:background="@drawable/tuoyuan"
            android:tag="module_006_title_icon"
            android:visibility="gone" />

        <TextView
            android:id="@+id/id_module_6_title"
            style="@style/ModuleTitleStyle"
            android:layout_width="@dimen/width_1600px"
            android:layout_height="@dimen/height_34px"
            android:layout_marginLeft="@dimen/width_6px"
            android:layout_marginTop="@dimen/height_47px"
            android:layout_toRightOf="@id/id_module_6_title_icon"
            android:includeFontPadding="false"
            android:tag="module_006_title"
            android:visibility="gone" />

        <!-- 1 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_6_view1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_6_title"
            app:block_poster_height="@dimen/height_46px"
            app:block_poster_holder="@drawable/focus_168_46"
            app:block_poster_width="@dimen/width_168px"
            app:block_tag="cell_006_1">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>


        <!-- 2 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_6_view2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_6_title"
            android:layout_toEndOf="@id/id_module_6_view1"
            android:layout_toRightOf="@id/id_module_6_view1"
            app:block_poster_height="@dimen/height_46px"
            app:block_poster_holder="@drawable/focus_168_46"
            app:block_poster_width="@dimen/width_168px"
            app:block_tag="cell_006_2">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 3 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_6_view3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_6_title"
            android:layout_toEndOf="@id/id_module_6_view2"
            android:layout_toRightOf="@id/id_module_6_view2"
            app:block_poster_height="@dimen/height_46px"
            app:block_poster_holder="@drawable/focus_168_46"
            app:block_poster_width="@dimen/width_168px"
            app:block_tag="cell_006_3">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 4 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_6_view4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_6_title"
            android:layout_toEndOf="@id/id_module_6_view3"
            android:layout_toRightOf="@id/id_module_6_view3"
            app:block_poster_height="@dimen/height_46px"
            app:block_poster_holder="@drawable/focus_168_46"
            app:block_poster_width="@dimen/width_168px"
            app:block_tag="cell_006_4">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 5 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_6_view5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_6_title"
            android:layout_toEndOf="@id/id_module_6_view4"
            android:layout_toRightOf="@id/id_module_6_view4"
            app:block_poster_height="@dimen/height_46px"
            app:block_poster_holder="@drawable/focus_168_46"
            app:block_poster_width="@dimen/width_168px"
            app:block_tag="cell_006_5">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 6 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_6_view6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_6_title"
            android:layout_toEndOf="@id/id_module_6_view5"
            android:layout_toRightOf="@id/id_module_6_view5"
            app:block_poster_height="@dimen/height_46px"
            app:block_poster_holder="@drawable/focus_168_46"
            app:block_poster_width="@dimen/width_168px"
            app:block_tag="cell_006_6">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 7 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_6_view7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_6_title"
            android:layout_toEndOf="@id/id_module_6_view6"
            android:layout_toRightOf="@id/id_module_6_view6"
            app:block_poster_height="@dimen/height_46px"
            app:block_poster_holder="@drawable/focus_168_46"
            app:block_poster_width="@dimen/width_168px"
            app:block_tag="cell_006_7">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 8 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_6_view8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_6_title"
            android:layout_toEndOf="@id/id_module_6_view7"
            android:layout_toRightOf="@id/id_module_6_view7"
            android:tag="cell_006_8"
            app:block_last="true"
            app:block_poster_height="@dimen/height_46px"
            app:block_poster_holder="@drawable/focus_168_46"
            app:block_poster_width="@dimen/width_168px">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

    </RelativeLayout>
</RelativeLayout>

