<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">

        <ImageView
            android:id="@+id/id_module_27_title_icon"
            android:layout_width="@dimen/width_17px"
            android:layout_height="@dimen/height_17px"
            android:layout_marginLeft="@dimen/width_20px"
            android:layout_marginTop="@dimen/height_56px"
            android:background="@drawable/tuoyuan"
            android:tag="module_027_title_icon"
            android:visibility="gone" />

        <TextView
            android:id="@+id/id_module_27_title"
            style="@style/ModuleTitleStyle"
            android:layout_width="@dimen/width_1600px"
            android:layout_height="@dimen/height_34px"
            android:layout_marginLeft="@dimen/width_6px"
            android:layout_marginTop="@dimen/height_47px"
            android:layout_toRightOf="@id/id_module_27_title_icon"
            android:includeFontPadding="false"
            android:tag="module_027_title"
            android:visibility="visible" />

        <!-- 1 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_27_view1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_27_title"
            android:layout_marginTop="@dimen/height_15px"
            app:block_poster_height="@dimen/height_240px"
            app:block_poster_holder="@drawable/focus_528_240"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_027_1">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>


        <!-- 2 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_27_view2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_27_title"
            android:layout_marginTop="@dimen/height_15px"
            android:layout_toEndOf="@id/id_module_27_view1"
            android:layout_toRightOf="@id/id_module_27_view1"
            app:block_poster_height="@dimen/height_240px"
            app:block_poster_holder="@drawable/focus_528_240"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_027_2">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 3 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_27_view3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_27_title"
            android:layout_marginTop="@dimen/height_15px"
            android:layout_toEndOf="@id/id_module_27_view2"
            android:layout_toRightOf="@id/id_module_27_view2"
            app:block_poster_height="@dimen/height_240px"
            app:block_poster_holder="@drawable/focus_528_240"
            app:block_poster_width="@dimen/width_528px"
            app:block_last="true"
            app:block_tag="cell_027_3">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 4 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_27_view4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_27_view1"
            app:block_poster_height="@dimen/height_240px"
            app:block_poster_holder="@drawable/focus_240_240"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_027_4">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 5 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_27_view5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_27_view1"
            android:layout_toEndOf="@id/id_module_27_view4"
            android:layout_toRightOf="@id/id_module_27_view4"
            app:block_poster_height="@dimen/height_240px"
            app:block_poster_holder="@drawable/focus_240_240"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_027_5">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 6 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_27_view6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_27_view2"
            android:layout_toEndOf="@id/id_module_27_view5"
            android:layout_toRightOf="@id/id_module_27_view5"
            app:block_poster_height="@dimen/height_240px"
            app:block_poster_holder="@drawable/focus_528_240"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_027_6">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 7 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_27_view7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_27_view3"
            android:layout_toEndOf="@id/id_module_27_view6"
            android:layout_toRightOf="@id/id_module_27_view6"
            app:block_poster_height="@dimen/height_240px"
            app:block_poster_holder="@drawable/focus_528_240"
            app:block_poster_width="@dimen/width_528px"
            app:block_last="true"
            app:block_tag="cell_027_7">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

    </RelativeLayout>
</RelativeLayout>

