<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="@dimen/height_345px"
    android:background="@drawable/dialog_bg"
    >

    <TextView
        android:id="@+id/text_view"
        android:layout_width="@dimen/width_900px"
        android:layout_height="@dimen/height_45px"
        android:layout_marginTop="@dimen/height_120px"
        android:text="播放已结束 (5S)"
        android:textColor="@color/colorWhite"
        android:gravity="center"
        android:textSize="@dimen/height_36px"
        />

    <TextView
        android:id="@+id/ensure"
        android:layout_width="@dimen/width_200px"
        android:layout_height="@dimen/height_60px"
        android:layout_marginTop="@dimen/height_216px"
        android:layout_centerHorizontal="true"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:gravity="center"
        android:text="确定"
        android:textColor="@color/colorWhite"
        android:textSize="@dimen/height_36px"
        android:background="@drawable/mask"
        />



</RelativeLayout>