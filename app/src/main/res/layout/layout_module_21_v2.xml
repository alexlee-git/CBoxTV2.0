<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">

        <ImageView
            android:id="@+id/id_module_21_title_icon"
            android:layout_width="@dimen/width_17px"
            android:layout_height="@dimen/height_17px"
            android:layout_marginLeft="@dimen/width_20px"
            android:layout_marginTop="@dimen/height_68px"
            android:background="@drawable/tuoyuan"
            android:tag="module_021_title_icon"
            android:visibility="gone" />

        <TextView
            android:id="@+id/id_module_21_title"
            style="@style/ModuleTitleStyle"
            android:layout_width="@dimen/width_1600px"
            android:layout_height="@dimen/height_34px"
            android:layout_marginLeft="@dimen/width_8px"
            android:layout_marginTop="@dimen/height_59px"
            android:layout_toRightOf="@id/id_module_21_title_icon"
            android:includeFontPadding="false"
            android:tag="module_021_title"
            android:visibility="gone" />

        <!-- 1 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:layout_marginTop="@dimen/height_15px"
            android:id="@+id/id_module_21_view1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_21_title"
            app:block_poster_height="@dimen/height_296px"
            app:block_poster_holder="@drawable/focus_528_296"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_021_1">

            <!-- 左上角角标 -->
            <tv.newtv.cboxtv.views.custom.RecycleImageView
                android:layout_width="@dimen/width_51px"
                android:layout_height="@dimen/height_29px"
                android:layout_marginLeft="@dimen/width_45px"
                android:layout_marginStart="@dimen/width_45px"
                android:layout_marginTop="@dimen/height_45px" />

        </tv.newtv.cboxtv.views.custom.BlockPosterView>
        <!-- 2 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:layout_marginTop="@dimen/height_15px"
            android:id="@+id/id_module_21_view2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/id_module_21_title"
            android:layout_toEndOf="@+id/id_module_21_view1"
            android:layout_toRightOf="@+id/id_module_21_view1"
            app:block_poster_height="@dimen/height_296px"
            app:block_poster_holder="@drawable/focus_528_296"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_021_2">

            <!-- 左上角角标 -->
            <tv.newtv.cboxtv.views.custom.RecycleImageView
                android:layout_width="@dimen/width_51px"
                android:layout_height="@dimen/height_29px"
                android:layout_marginLeft="@dimen/width_45px"
                android:layout_marginStart="@dimen/width_45px"
                android:layout_marginTop="@dimen/height_45px" />

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 3 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:layout_marginTop="@dimen/height_15px"
            android:id="@+id/id_module_21_view3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/id_module_21_title"
            android:layout_toEndOf="@+id/id_module_21_view2"
            android:layout_toRightOf="@+id/id_module_21_view2"
            app:block_last="true"
            app:block_poster_height="@dimen/height_296px"
            app:block_poster_holder="@drawable/focus_528_296"
            app:block_poster_width="@dimen/width_528px"
            app:block_tag="cell_021_3">

            <!-- 左上角角标 -->
            <tv.newtv.cboxtv.views.custom.RecycleImageView
                android:layout_width="@dimen/width_51px"
                android:layout_height="@dimen/height_29px"
                android:layout_marginLeft="@dimen/width_45px"
                android:layout_marginStart="@dimen/width_45px"
                android:layout_marginTop="@dimen/height_45px" />

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 4 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_21_view4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_21_view1"
            app:block_poster_height="@dimen/height_360px"
            app:block_poster_holder="@drawable/focus_240_360"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_021_4">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 5 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_21_view5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_21_view1"
            android:layout_toEndOf="@id/id_module_21_view4"
            android:layout_toRightOf="@id/id_module_21_view4"
            app:block_poster_height="@dimen/height_360px"
            app:block_poster_holder="@drawable/focus_240_360"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_021_5">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>


        <!-- 6 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_21_view6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_21_view1"
            android:layout_toEndOf="@id/id_module_21_view5"
            android:layout_toRightOf="@id/id_module_21_view5"
            app:block_poster_height="@dimen/height_360px"
            app:block_poster_holder="@drawable/focus_240_360"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_021_6">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 7 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_21_view7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_21_view1"
            android:layout_toEndOf="@id/id_module_21_view6"
            android:layout_toRightOf="@id/id_module_21_view6"
            app:block_poster_height="@dimen/height_360px"
            app:block_poster_holder="@drawable/focus_240_360"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_021_7">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>


        <!-- 8 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_21_view8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_21_view1"
            android:layout_toEndOf="@id/id_module_21_view7"
            android:layout_toRightOf="@id/id_module_21_view7"
            app:block_poster_height="@dimen/height_360px"
            app:block_poster_holder="@drawable/focus_240_360"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_021_8">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

        <!-- 9 -->
        <tv.newtv.cboxtv.views.custom.BlockPosterView
            android:id="@+id/id_module_21_view9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_module_21_view1"
            android:layout_toEndOf="@id/id_module_21_view8"
            android:layout_toRightOf="@id/id_module_21_view8"
            app:block_last="true"
            app:block_poster_height="@dimen/height_360px"
            app:block_poster_holder="@drawable/focus_240_360"
            app:block_poster_width="@dimen/width_240px"
            app:block_tag="cell_021_9">

        </tv.newtv.cboxtv.views.custom.BlockPosterView>

    </RelativeLayout>
</RelativeLayout>

